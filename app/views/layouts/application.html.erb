<!DOCTYPE html>
<html>
<head>
  <title>Overboard</title>
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body class="page">
  <header>
    <div class="container">
      <%= render 'layouts/header' %>
    </div>
  </header>

  <main>
    <div class="container">
      <%= yield %>
    </div>
  </main>

  <footer>
    <div class="container">
      <%= render 'layouts/footer' %>
    </div>
  </footer>
</body>
</html>
